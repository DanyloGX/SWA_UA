    <Group Name="E_m2_03_es">
      <Cell Name="000">
        <Message MessageData="Ці стародавні сувої відомі як{NewLine}Манускрипти Гаї" />
        <ColourMain Start="0" End="57" Marker="2" Alpha="255" Red="255" Green="255" Blue="255" />
        <ColourSub1 Start="0" End="57" Marker="1" Alpha="0" Red="0" Green="0" Blue="28" />
        <ColourSub2 Start="0" End="57" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="010">
        <Message MessageData="Вони сповідують легенду про{NewLine}жах, що перенесла наша планета" />
        <ColourMain Start="0" End="56" Marker="2" Alpha="255" Red="255" Green="255" Blue="255" />
        <ColourSub1 Start="0" End="56" Marker="1" Alpha="0" Red="0" Green="0" Blue="28" />
        <ColourSub2 Start="0" End="56" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="020">
        <Message MessageData="декілька десятків тисяч років тому." />
        <ColourMain Start="0" End="35" Marker="2" Alpha="255" Red="255" Green="255" Blue="255" />
        <ColourSub1 Start="0" End="35" Marker="1" Alpha="0" Red="0" Green="0" Blue="28" />
        <ColourSub2 Start="0" End="35" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="025">
        <Message MessageData="" />
        <ColourMain Start="0" End="-1" Marker="2" Alpha="255" Red="0" Green="0" Blue="0" />
        <ColourSub1 Start="0" End="-1" Marker="1" Alpha="0" Red="0" Green="0" Blue="11" />
        <ColourSub2 Start="0" End="-1" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="030">
        <Message MessageData="Про який жах...?" />
        <ColourMain Start="0" End="13" Marker="2" Alpha="255" Red="255" Green="255" Blue="255" />
        <ColourSub1 Start="0" End="13" Marker="1" Alpha="0" Red="0" Green="0" Blue="28" />
        <ColourSub2 Start="0" End="13" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="040">
        <Message MessageData="Великий. І якщо вірити{NewLine}манускриптам..." />
        <ColourMain Start="0" End="46" Marker="2" Alpha="255" Red="255" Green="255" Blue="255" />
        <ColourSub1 Start="0" End="46" Marker="1" Alpha="0" Red="0" Green="0" Blue="28" />
        <ColourSub2 Start="0" End="46" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="050">
        <Message MessageData="це не перший раз, коли планету{NewLine}розкололо на декілька частин." />
        <ColourMain Start="0" End="70" Marker="2" Alpha="255" Red="255" Green="255" Blue="255" />
        <ColourSub1 Start="0" End="70" Marker="1" Alpha="0" Red="0" Green="0" Blue="28" />
        <ColourSub2 Start="0" End="70" Marker="0" Alpha="0" Red="0" Green="0" Blue="1" />
      </Cell>
      <Cell Name="055">
